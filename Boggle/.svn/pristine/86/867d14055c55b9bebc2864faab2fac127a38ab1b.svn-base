<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="BoggleClient.MainWindow"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="289*"/>
            <ColumnDefinition Width="228*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="NameTextbox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="Enter Name..." VerticalAlignment="Top" Width="276" Background="White" TextChanged="NameTextbox_TextChanged"/>
        <TextBox x:Name="IPTextbox" HorizontalAlignment="Left" Height="23" Margin="10,38,0,0" TextWrapping="Wrap" Text="Enter IP Address..." VerticalAlignment="Top" Width="276" TextChanged="IPTextbox_TextChanged"/>
        <Button x:Name="PlayButton" Content="Play" HorizontalAlignment="Left" Margin="10,99,0,0" VerticalAlignment="Top" Width="116" Height="22" Click="PlayButton_Click"/>

    </Grid>
</Window>
